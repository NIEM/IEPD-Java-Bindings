      </xs:enumeration>
      <xs:enumeration value="Unknown">
        <xs:annotation>
          <xs:documentation>Severity unknown</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="AlertEventDetailsSeverityCodeType">
    <xs:annotation>
      <xs:documentation>A data type for the severity code designated to an alert event</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="em:AlertEventDetailsSeverityCodeSimpleType">
