  <xs:complexType name="AlertEventDetailsType">
    <xs:annotation>
      <xs:documentation>A data type for an alert event details</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="structures:ObjectType">
        <xs:sequence>
          <xs:element ref="em:AlertEventDetailsAudienceDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="em:AlertEventDetailsSystemRecordID" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="em:AlertEventDetailsDescriptionText" minOccurs="0" maxOccurs="unbounded"/>
